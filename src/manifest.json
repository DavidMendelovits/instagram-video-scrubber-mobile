{
  "name": "trainingstage",
  "manifest_version": 2,
  "version": "0.1",
  "permissions": [
    "https://www.instagram.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [{
    "matches": [
      "https://www.instagram.com/*"
    ],
    "js": [
      "contentScript.js"
    ]
  }],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
